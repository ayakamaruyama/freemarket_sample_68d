%li.pickUp__content
  = link_to item_path(item.id), class: "pickUp__contentLink" do
    - item.item_images.first(1).each do |item_image|
      = image_tag item_image.url, alt: "item3", class: "pickUp__itemImage"
    .pickUp__details
      .pickUp__itemDetails
        %h3.pickUp__name= item.name
        %p.pickUp__price= item.price
        %p.pickUp__taxInfo (税込)
      .pickUp__favBox
        %i.fa.fa-star
        %p.pickUp__favNumber 
    